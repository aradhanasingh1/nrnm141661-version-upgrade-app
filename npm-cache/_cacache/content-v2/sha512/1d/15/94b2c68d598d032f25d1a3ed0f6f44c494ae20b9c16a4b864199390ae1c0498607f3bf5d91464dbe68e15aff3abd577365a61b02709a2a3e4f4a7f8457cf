rate process\n\n```js\n// next.config.js\nconst withTypescript = require(\"@zeit/next-typescript\")\nconst ForkTsCheckerWebpackPlugin = require('fork-ts-checker-webpack-plugin');\n\nmodule.exports = withTypescript({\n  webpack(config, options) {\n    // Do not run type checking twice:\n    if (options.isServer) config.plugins.push(new ForkTsCheckerWebpackPlugin())\n    \n    return config\n  }\n})\n```\n","gitHead":"cfbb765bc565c59b0c9f3f9fe5fd1c72156f0a39","_npmUser":{"name":"timneutkens","email":"tim@timneutkens.nl"},"repository":{"url":"git+https://github.com/zeit/next-plugins.git","type":"git"},"_npmVersion":"6.1.0","description":"Use [Typescript](https://www.typescriptlang.org/) with [Next.js](https://github.com/zeit/next.js)","directories":{},"_nodeVersion":"10.7.0","dependencies":{"@babel/preset-typescript":"7.0.0-beta.42"},"_hasShrinkwrap":false,"readmeFilename":"readme.md","_npmOperationalInternal":{"tmp":"tmp/next-typescript_1.1.1-canary.1_1532950313583_0.9574558718983053","host":"s3://npm-registry-packages"}},"1.1.1-canary.2":{"name":"@zeit/next-typescript","version":"1.1.1-canary.2","license":"MIT","_id":"@zeit/next-typescript@1.1.1-canary.2","maintainers":[{"name":"arunoda","email":"arunoda.susiripala@gmail.com"},{"name":"codetheory","email":"thecodetheory@gmail.com"},{"name":"hharnisc","email":"hharnisc@gmail.com"},{"name":"iamevilrabbit","email":"hello@evilrabb.it"},{"name":"igorklopov","email":"igor@klopov.com"},{"name":"jamo","email":"jamo@isotalo.fi"},{"name":"javivelasco","email":"javier.velasco86@gmail.com"},{"name":"leo","email":"leo@zeit.co"},{"name":"matheuss","email":"me@matheus.top"},{"name":"nkzawa","email":"naoyuki.kanezawa@gmail.com"},{"name":"olliv","email":"olli@zeit.co"},{"name":"rauchg","email":"rauchg@gmail.com"},{"name":"sergiodxa","email":"sergiodxa@gmail.com"},{"name":"timneutkens","email":"tim@timneutkens.nl"},{"name":"tootallnate","email":"nathan@tootallnate.net"}],"homepage":"https://github.com/zeit/next-plugins#readme","bugs":